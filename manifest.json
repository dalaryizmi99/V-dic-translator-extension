{
  "manifest_version": 3,
  "name": "V-Dic Translator",
  "version": "1.0.0",
  "description": "Tech the world how to translate Vietnamese from English",
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
      "https://translate.google.com/"
  ],
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ]
}